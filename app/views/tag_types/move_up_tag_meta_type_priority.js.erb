  $("#div_<%= escape_javascript("#{@up_current_tag_meta_type.position+1}")%> a").find("img").attr("src","/images/down_arrow.png").attr("alt","Down_arrow");;
  var temp = $("#div_<%= escape_javascript("#{@up_current_tag_meta_type.position+1}")%>").html();
  $("#div_<%= escape_javascript("#{@up_current_tag_meta_type.position}")%>").find("a").attr("title","Move Up").attr("href","/tag_types/<%= escape_javascript("#{@up_exchange_tag_meta_type.id}")%>/move_up_tag_meta_type_priority");
