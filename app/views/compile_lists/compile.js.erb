<%- unless flash[:error].nil? %>
$("#flash_message").html("<%= escape_javascript(flash[:error]) %>");
$('#flash_message').dialog( {
    modal: true,
      resizable: false,
    draggable: true,
      height: 'auto',
      width: 'auto',
    <%#*position: 'top',%>
    buttons: {
      Ok: function() {
        $(this).dialog('close');
      }
    }
  });


   $('#flash_message').parent().find("a").css("display","none");
   $("#flash_message").parent().css('background-color','#D1DDE6');
   $("#flash_message").css('background-color','#D1DDE6');

$('#flash_message').dialog('option', 'title', 'Error');
$('#flash_message').dialog('open');
<%- else %>

$('#compile_result').html('<%= escape_javascript(render(:partial => "lists/compile_results_template", :locals => {:top_partial=>"person_compile_list_template_title",:list_type=>"person"})) %>');
$('#compile_result_div').dialog({
  modal: true,
  position: 'center',
  resizable: true,
  draggable: true,
  height: 700,
  width: 1100

});
$('#compile_result_div').dialog('option', 'title', 'Compile Result');
$('#compile_result_div').parent().css('background-color','#D1DDE6');
$('#compile_result_div').css('background-color','#D1DDE6');
$('#compile_result_div').dialog('open');



<%if  !(@check_list_empty.size >0)%>
$('#list_header_name').attr('disabled',true);
$('#list_header_description').attr('disabled',true);
<%end%>



<%- end %>

$('#compile_button').removeAttr('disabled');
$('#spinner').remove();