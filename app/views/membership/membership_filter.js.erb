
<%- if @date_valid%>

  <%- if @state == "end_cycle" %>
    $("#existing_end_cycle_membership" ).show().html('<%=escape_javascript(render(:partial => "membership/display_grid", :locals => {:membership_status=>"Rejected"})) %>');

  <%-elsif @state == "life"%>
    $('#life_grid').show().html('<%= escape_javascript(render :partial => "/membership/display_grid",:locals=>{:membership_status=>"Actived"}) %>');
    
  <%elsif @state =="review"%>
    $('#review_page_grid').show().html('<%= escape_javascript(render :partial => "/membership/display_grid",:locals=>{:membership_status=>"Prospective"}) %>');

  <%end%>

<%- else %>


  $("#flash_message_text").html("<%=escape_javascript(flash[:error])%>");
  $('#flash_message').dialog( {
  modal: true,
  resizable: false,
  draggable: true,
  height: 'auto',
  width: 600,
  buttons:{
  Ok: function() {
  $(this).dialog('destroy');
  }
  }
  });
  $('#flash_message').dialog( 'option', 'title', 'ERROR');
  $('#flash_message').parent().find("a").css("display","none");
  $("#flash_message").parent().css('background-color','#D1DDE6');
  $("#flash_message").css('background-color','#D1DDE6');
  $('#flash_message').dialog('open');
<%- end %>

enable_form_after_submit_finish();


