<% if flash[:error].nil? -%>
  $('.close_option[field="organisation_relationship"]').click();
  $('#existing_relationships').html('<%= escape_javascript(render(:partial => '/organisation_relationships/existing_branches', :locals =>{:level => @next_level})) %>');
  $('#organisation_relationship_mode').attr('mode', 'show');
<%else%>
  $("#flash_message_text").html("<%=escape_javascript(flash[:error])%>");
  $("#flash_message").dialog( {
  modal:true,
  resizable: false,
  height: 'auto',
  width: 'auto',
  buttons: {
  OK: function(){
  $(this).dialog('close');
  }
  }
  });


  $('#flash_message').parent().find("a").css("display","none");
  $("#flash_message").parent().css('background-color','#D1DDE6');
  $("#flash_message").css('background-color','#D1DDE6');
  $('#flash_message').dialog('open');
  $('#flash_message').dialog('option', 'title', 'Error');

<%end%>
enable_form_after_submit_finish();
