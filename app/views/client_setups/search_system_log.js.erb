
<%-if flash[:error].nil?%>
$(#system_log_search_results).html('<%= escape_javascript(render(:partial => "/client_setups/system_log_search_result")) %>')
<%-else%>

$("#flash_message_text").html("<%=escape_javascript(flash[:error])%>");
 $('#flash_message').dialog( {
     modal: true,
      resizable: false,
      draggable: true,
      height: 'auto',
      width: 600,
  buttons:{
  Ok: function() {
  $(this).dialog('destroy');
  }
  }
  });
 $('#flash_message').dialog( 'option', 'title', 'ERROR');
