- case session[:module]
  - when "dashboard"
    = render :partial => "/shared/dashboard_module"
  - when "core"
    = render :partial => "/shared/core_module"
  - when "receipting"
    = render :partial => "/shared/receipting_module"
  - when "administration"
    = render :partial => "/shared/administration_module"
  - when "client_setup"
    = render :partial => "/shared/client_setup_module"
  - when "membership"
    = render :partial => "/shared/membership_module"
  - else
    = render :partial => "/shared/core_module"
