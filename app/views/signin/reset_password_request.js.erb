if ("<%= escape_javascript(@password_reset) %>" == "true") {
  $("#login_password_reset_security_questions").replaceWith("<%= escape_javascript(render(:partial => "/signin/password_reset_success"))%>");
} else {
  $("#login_password_reset_security_questions").replaceWith("<%= escape_javascript(render(:partial => "/signin/password_reset_fail"))%>");
}